{
    "name": "ubccr/ralph",
    "description": "A simple SPARQL client built on top of Guzzle.",
    "type": "library",
    "keywords": ["rdf", "sparql"],
    "homepage": "https://github.com/ubccr/ralph",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Center for Computational Research, University at Buffalo. The State University of New York",
            "email": "ccr@buffalo.edu"
        },
        {
            "name": "John Rivera",
            "email": "riveraj@buffalo.edu"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "CCR\\Ralph\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CCR\\Ralph\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
